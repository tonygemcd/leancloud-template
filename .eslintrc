{
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "plugins": [
    "html"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "__COVERAGE__": false,
    "__BASENAME__": false,
    "$request": false,
    "log": false
  },
  "rules": {
    "arrow-parens": 0
  }
}
